{
  "name": "bun-hot-build-web",
  "module": "index.ts",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_ENV=development bun run --hot index.ts",
    "start": "NODE_ENV=production bun run index.ts",
    "build": "NODE_ENV=production bun build index.ts --target=bun --minify --outfile=dist/index.js"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
